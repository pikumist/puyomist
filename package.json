    "comlink": "^4.4.1",
    "framer-motion": "^11.3.2",
    "p-limit": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.2.1",
    "react-redux": "^9.1.2"
